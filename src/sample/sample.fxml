<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <AnchorPane prefHeight="358.0" prefWidth="309.0">
         <children>
            <Label fx:id="showUserNameLabel" layoutX="88.0" layoutY="41.0" prefHeight="17.0" prefWidth="127.0" text="Username shown here" textAlignment="CENTER" />
            <Button fx:id="connectButton" layoutX="88.0" layoutY="83.0" mnemonicParsing="false" onAction="#connectButton" prefHeight="25.0" prefWidth="127.0" text="Connect" />
            <Label layoutX="14.0" layoutY="133.0" prefHeight="25.0" prefWidth="66.0" text="Username" />
            <Label layoutX="15.0" layoutY="164.0" prefHeight="25.0" prefWidth="60.0" text="Password" />
            <Button fx:id="loginButton" layoutX="26.0" layoutY="226.0" mnemonicParsing="false" onAction="#loginButtonOnAction" text="Login" />
            <Button fx:id="cancelButton" layoutX="169.0" layoutY="225.0" mnemonicParsing="false" onAction="#cancelButtonOnAction" text="Cancel" />
            <Label fx:id="loginMessageLabel" layoutX="100.0" layoutY="199.0" prefHeight="17.0" prefWidth="138.0" textAlignment="CENTER" textFill="#c41a1a">
               <font>
                  <Font size="10.0" />
               </font></Label>
            <TextField fx:id="usernameTextField" layoutX="95.0" layoutY="133.0" />
            <PasswordField fx:id="enterPasswordField" layoutX="95.0" layoutY="167.0" />
         </children>
      </AnchorPane>
   </children>
</GridPane>
